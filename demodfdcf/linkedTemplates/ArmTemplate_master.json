{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "demodfdcf"
		},
		"linkedservicedL_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linkedservicedL'"
		},
		"linked_git_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"linkedservicedL_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://demostorageaccountdcf.dfs.core.windows.net/"
		},
		"managerTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/f592e323-15e5-4524-80d7-4cb862ee9284/resourceGroups/RG-DemoDCF/providers/Microsoft.Storage/storageAccounts/demostorageaccountdcf"
		},
		"trigger_manager_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/f592e323-15e5-4524-80d7-4cb862ee9284/resourceGroups/RG-DemoDCF/providers/Microsoft.Storage/storageAccounts/demostorageaccountdcf"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "demodfdcf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"linkedservicedL_accountKey": {
						"value": "[parameters('linkedservicedL_accountKey')]"
					},
					"linked_git_properties_typeProperties_url": {
						"value": "[parameters('linked_git_properties_typeProperties_url')]"
					},
					"linkedservicedL_properties_typeProperties_url": {
						"value": "[parameters('linkedservicedL_properties_typeProperties_url')]"
					},
					"managerTrigger_properties_typeProperties_scope": {
						"value": "[parameters('managerTrigger_properties_typeProperties_scope')]"
					},
					"trigger_manager_properties_typeProperties_scope": {
						"value": "[parameters('trigger_manager_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}